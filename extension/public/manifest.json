{
  "name": "Cyber-Secure",
  "description": "An extension helps you secure from script attacks",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "CyberSecure",
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "128": "logo192.png"
    }
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "storage",
    "notifications",
    "activeTab", "scripting"
  ],
  "omnibox": {
    "keyword": "safecheck"
  }
}